@model IEnumerable<ELearningUI.Models.Calender>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CalenderName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Enrollment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Program)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CalenderName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Enrollment.EnrollmentId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Program.ProgramId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CalenderId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CalenderId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CalenderId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
