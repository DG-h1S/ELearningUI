@model ELearningUI.Models.ProgramType

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>ProgramType</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ProgramName" class="control-label">Program Name</label>
                <input asp-for="ProgramName" class="form-control" />
                <span asp-validation-for="ProgramName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProgramCode" class="control-label">Program Code</label>
                <input asp-for="ProgramCode" class="form-control" />
                <span asp-validation-for="ProgramCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <a class="btn btn-outline-secondary"asp-action="Index">Cancel</a>
            </div>
        </form>
    </div>
</div>

<div>
   
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
